<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="483.0" prefWidth="715.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UI">
   <children>
      <Label layoutX="260.0" layoutY="22.0" text="Trie Tree">
         <font>
            <Font name="Times New Roman Bold" size="48.0" />
         </font>
      </Label>
      <TextArea fx:id="text" layoutX="61.0" layoutY="122.0" prefHeight="120.0" prefWidth="280.0" />
      <Label layoutX="183.0" layoutY="84.0" text="Text">
         <font>
            <Font name="Times New Roman" size="20.0" />
         </font>
      </Label>
      <TextField fx:id="word" layoutX="52.0" layoutY="317.0" />
      <Label layoutX="104.0" layoutY="282.0" text="Word">
         <font>
            <Font name="Times New Roman" size="20.0" />
         </font>
      </Label>
      <Button layoutX="73.0" layoutY="364.0" mnemonicParsing="false" onAction="#insert_button" text="Insert">
         <font>
            <Font name="Times New Roman" size="12.0" />
         </font>
      </Button>
      <Button layoutX="136.0" layoutY="364.0" mnemonicParsing="false" onAction="#remove_button" text="Remove">
         <font>
            <Font name="Times New Roman" size="12.0" />
         </font>
      </Button>
      <Button layoutX="319.0" layoutY="434.0" mnemonicParsing="false" onAction="#reset" text="Reset Word">
         <font>
            <Font name="Times New Roman" size="12.0" />
         </font>
      </Button>
      <Label fx:id="result" alignment="CENTER" layoutX="430.0" layoutY="122.0" prefHeight="120.0" prefWidth="206.0" textAlignment="CENTER">
         <font>
            <Font name="Times New Roman" size="15.0" />
         </font>
      </Label>
      <Button layoutX="374.0" layoutY="242.0" mnemonicParsing="false" onAction="#check" text="Check" />
      <Label fx:id="description" alignment="CENTER" layoutX="430.0" layoutY="257.0" prefHeight="120.0" prefWidth="206.0" textAlignment="CENTER">
         <font>
            <Font name="Times New Roman" size="15.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
